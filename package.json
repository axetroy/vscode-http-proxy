{
  "name": "vscode-http-proxy",
  "displayName": "Http Proxy",
  "description": "Start HTTP proxy server",
  "version": "0.1.3",
  "publisher": "axetroy",
  "engines": {
    "vscode": "^1.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vscode-http-proxy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axetroy/vscode-http-proxy/issues"
  },
  "homepage": "https://github.com/axetroy/vscode-http-proxy#readme",
  "keywords": [
    "server",
    "serve",
    "static"
  ],
  "categories": [
    "Other"
  ],
  "icon": "resources/icon.png",
  "main": "./out/index.js",
  "activationEvents": [
    "onCommand:http-proxy.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "http-proxy.start",
        "title": "%cmd.start.title%",
        "category": "http-proxy"
      },
      {
        "command": "http-proxy.stop",
        "title": "%cmd.stop.title%",
        "category": "http-proxy",
        "icon": {
          "light": "./resources/light/stop.svg",
          "dark": "./resources/dark/stop.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "http-proxy.stop",
          "when": "HTTP_PROXY_SERVER_RUNNING",
          "group": "navigation@1"
        }
      ],
      "editor/title/context": [
        {
          "command": "http-proxy.stop",
          "when": "HTTP_PROXY_SERVER_RUNNING",
          "group": "http-proxy"
        }
      ],
      "commandPalette": [
        {
          "command": "http-proxy.start",
          "when": "!HTTP_PROXY_SERVER_RUNNING"
        },
        {
          "command": "http-proxy.stop",
          "when": "HTTP_PROXY_SERVER_RUNNING"
        }
      ]
    },
    "configuration": {
      "title": "%ext.config.title%",
      "properties": {
        "http-proxy.port": {
          "type": "integer",
          "default": 8080,
          "markdownDescription": "%ext.config.port%"
        },
        "http-proxy.target": {
          "type": "string",
          "default": "",
          "markdownDescription": "%ext.config.target%"
        },
        "http-proxy.cors": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%ext.config.cors%"
        },
        "http-proxy.headers": {
          "type": "object",
          "markdownDescription": "%ext.config.headers%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rimraf ./out",
    "compile": "npm run clean && npm run lint && tsc -p ./ && npx @zeit/ncc build out/index.js -o ./dist -e vscode -m && npm run clean && move-cli ./dist ./out",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "tslint -p ./ -c tslint.json",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish": "npx vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "11.15.0",
    "@types/open": "6.1.0",
    "move-cli": "1.2.1",
    "rimraf": "3.0.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4",
    "vscode": "1.1.36"
  },
  "dependencies": {
    "get-port": "^5.0.0",
    "http-proxy": "^1.17.0",
    "internal-ip": "^5.0.0",
    "vscode-nls-i18n": "^0.1.1"
  }
}
